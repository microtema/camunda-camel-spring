        <camunda:inputOutput>
          <camunda:inputParameter name="inputFileName">purchase-order.csv</camunda:inputParameter>
          <camunda:inputParameter name="fileName">messages.xml</camunda:inputParameter>
          <camunda:inputParameter name="input_map">
            <camunda:map>
              <camunda:entry key="fileName">foo.bar</camunda:entry>
            </camunda:map>
          </camunda:inputParameter>
          <camunda:inputParameter name="Input_List">
            <camunda:list>
              <camunda:value>one</camunda:value>
              <camunda:value>0</camunda:value>
              <camunda:value>true</camunda:value>
              <camunda:value>java.lang.String</camunda:value>
              <camunda:value>null</camunda:value>
            </camunda:list>
          </camunda:inputParameter>
          <camunda:outputParameter name="endpoint_output">seda:outputFile</camunda:outputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
    </bpmn:serviceTask>
    <bpmn:sequenceFlow id="SequenceFlow_from_start_to_read_task" sourceRef="StartEvent_1" targetRef="read-purchase-order-flatfile" />
